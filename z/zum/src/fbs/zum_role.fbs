include "zfb_types.fbs";
namespace Zum.fbs;
table RoleID {
  name:string;
}
table Role {
  name:string (key);
  perms:Zfb.Bitmap;	// bitmap
  apiperms:Zfb.Bitmap;	// API permissions
  flags:uint8;
}
table RoleUpdAck {
  role:Role;
  ok:uint8;
}
table RoleAck {
  ok:uint8;
}
table RoleList {
  list:[Role];
}
